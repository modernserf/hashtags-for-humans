{
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "ecmaFeatures": {
        "arrowFunctions":true,
        "blockBindings": true,
        "classes":true,
        "defaultParams":true,
        "destructuring":true,
        "forOf": true,
        "modules":true,
        "spread":true,
        "templateStrings":true,
        "jsx": true
    },
    "env": {
        "es6":true,
        "browser": true
    },
    "rules": {
        "strict": 0,
        "quotes": false,
        "space-infix-ops": false,
        "no-use-before-define": false,
        "no-console": false,
        "no-underscore-dangle": false,
        "camelcase": false,
        "new-cap": false,
        "no-multi-spaces": false,
        "comma-spacing": false,
        "key-spacing": false,
        "no-shadow": true,
        "global-strict": false,
        "comma-dangle": false,
        "no-extra-strict":false,
        "yoda":true,
        "no-empty":false
    }
}
